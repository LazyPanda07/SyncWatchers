{
  "ContentExecutor": {
    "loadType": "initialization",
    "route": "content/{string:room_uuid}",
    "api": "cxx"
  },
  "UploadContentExecutor": {
    "loadType": "dynamic",
    "route": "upload/{string:room_uuid}/{string:user_uuid}/{string:content_name}",
    "api": "cxx"
  },
  "DownloadContentExecutor": {
    "loadType": "dynamic",
    "route": "download/{string:room_uuid}/{string:content_name}",
    "api": "cxx"
  }
}